dockerpsqlexec() {
    if [ "$1" != "" ] && [ "$2" != "" ] && [ "$3" != "" ] && [ "$4" != "" ]
    then
        sudo docker cp ./$1 $2:/$4/$1
        sudo docker exec -u postgres $2 psql $3 postgres -f  $4/$1
    else
        echo "file, docker container id, dbname, and folder name are required"
    fi
}
